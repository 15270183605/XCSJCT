    <declare-styleable name="TabIndicator">
        <attr format="integer" name="visiabel_size"/> <!--可视化个数，比如有一排，我们就只要显示4个-->
        <attr format="color|reference" name="tab_color"/> <!--指示器的颜色-->
        <attr format="dimension" name="tab_width"/>  <!--指示器的宽度-->
        <attr format="dimension" name="tab_height"/> <!--指示器的高度-->
        <attr format="dimension" name="tab_textsize"/> <!--顶部文字的大小-->
        <attr format="color|reference" name="tab_text_default_color"/> <!--顶部文字默认的颜色-->
        <attr format="color|reference" name="tab_text_change_color"/> <!--移动时，顶部文字的颜色-->
        <attr format="boolean" name="tab_show"/>  <!--是否显示指示器-->
        <attr name="tab_text_type">               <!--顶部文字的类型，nromaltext为普通的textview，
                                                       corlortext为文件渐变-->
            <enum name="normaltext" value="0"/>
            <enum name="colortext" value="1"/>
        </attr>
        <attr name="tap_type">           <!--指示器类型，有三角形或者圆条-->
            <enum name="tri" value="0"/>
            <enum name="rect" value="1"/>
        </attr>
        <attr format="boolean" name="tab_iscanscroll"/>
    </declare-styleable>
    <declare-styleable name="TextIndicator">
        <attr format="boolean" name="word_show_circle"/> <!--是否显示外援的圆圈-->
        <attr format="reference|color" name="word_circle_color"/> <!--圆圈的颜色-->
        <attr format="reference|color" name="word_text_color"/> <!--文字颜色-->
        <attr format="dimension" name="word_text_size"/> <!--文字大小-->
    </declare-styleable>
    <declare-styleable name="TransIndicator">
        <attr format="dimension" name="trans_width"/> <!--指示器的大小-->
        <attr format="dimension" name="trans_height"/> <!--指示器的大小-->
        <attr format="color|reference" name="trans_defaultcolor"/> <!--指示器默认的颜色-->
        <attr format="dimension" name="trans_leftmargin"/>
        <attr format="color|reference" name="trans_movecolor"/> <!--指示器移动的颜色-->
        <attr format="boolean" name="trans_dismiss_open"/>
        <attr format="dimension" name="trans_round_radius"/>
        <attr name="trans_type">
            <enum name="circle" value="0"/>
            <enum name="round" value="1"/>
        </attr>
    </declare-styleable>
